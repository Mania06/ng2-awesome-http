{
  "name": "ng2-awesome-http",
  "private": false,
  "version": "2.0.0",
  "description": "Angular2 Http library that add some features to default http client",
  "scripts": {
    "clean": "rimraf dist/",
    "build:aot": "ngc -p tsconfig.json",
    "build:jit": "tsc -p tsconfig.json",
    "build": "npm run clean && npm run build:aot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csailly/ng2-awesome-http.git"
  },
  "author": {
    "name": "Christophe Sailly",
    "email": "christophe.sailly@gmail.com"
  },
  "keywords": [
    "angular",
    "angular2",
    "ng2",
    "http",
    "cache",
    "interceptor"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csailly/ng2-awesome-http/issues"
  },
  "engines": {
    "node": ">= 5.4.1"
  },
  "contributors": [],
  "main": "bundles/ng2-awesome-http.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "core-js": "2.4.1",
    "rimraf": "^2.5.4",
    "rxjs": "~5.3.0",
    "typescript": "2.2.2",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@types/jasmine": "2.2.30",
    "@types/node": "6.0.54"
  }
}
